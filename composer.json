{
  "name": "apie/text-value-objects",
  "description": "Composer package of the apie library: text value objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\TextValueObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\TextValueObjects\\": "tests/"
    }
  },
  "extra": {
    "apie-objects": [
      "Apie\\TextValueObjects\\CompanyName",
      "Apie\\TextValueObjects\\EncryptedPassword",
      "Apie\\TextValueObjects\\FirstName",
      "Apie\\TextValueObjects\\LastName",
      "Apie\\TextValueObjects\\NonEmptyString",
      "Apie\\TextValueObjects\\SmallDatabaseText",
      "Apie\\TextValueObjects\\StrongPassword"
    ]
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/count-words": "self.version"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "phpunit/phpunit": "^9.6.19",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}